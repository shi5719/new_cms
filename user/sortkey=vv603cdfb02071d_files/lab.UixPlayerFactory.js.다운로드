var UixPlayerFactory = {
	
	createPlayer: function() {

		if (bizDevice.isMobile()) {
			console.log('create UixPlayerMobile');
			return new UixPlayerMobile();
		}
		else {
            console.log('create UixPlayerDesktop');
			return new UixPlayerDesktop();
		}
	},
};







